<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="controllers.PosterController"
      prefHeight="400.0" prefWidth="540.0"
      alignment="CENTER">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="FOLK CUISINE FAIR" alignment="CENTER">
        <font>
            <Font name="Verdana" size="24.0"/>
        </font>
    </Label>

    <Label text="In your town" alignment="CENTER">
        <font>
            <Font name="Verdana" size="18.0"/>
        </font>
    </Label>

    <Label text="on Sunday, 08.08.2022" alignment="CENTER">
        <font>
            <Font name="Verdana" size="14.0"/>
        </font>
    </Label>

    <Label text="The list of participants" alignment="CENTER">
        <font>
            <Font name="Verdana" size="14.0"/>
        </font>
    </Label>

    <ScrollPane prefWidth="200.0" prefHeight="300">
        <GridPane fx:id="participants" alignment="CENTER" vgap="10.0" hgap="10.0">



        </GridPane>
    </ScrollPane>
</VBox>

